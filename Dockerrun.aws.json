{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "${DOCKERHUB_USERNAME}/${DOCKERHUB_IMAGE_NAME}:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 5000
        }
      ],
      "environment": [
        {
          "name": "TZ",
          "value": "Asia/Seoul"
        }
      ]
    },
    {
      "name": "redis",
      "image": "redis:alpine",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "containerPort": 6379,
          "hostPort": 6379
        }
      ],
      "environment": [
        {
          "name": "TZ",
          "value": "Asia/Seoul"
        }
      ]
    }
  ]
}
